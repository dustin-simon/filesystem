{
    "name": "dustin/filesystem",
    "type": "library",
    "description": "A Symfony bundle to manage filesystems with the League Flysystem",
    "keywords": [
        "filesystem",
        "symfony-bundle",
        "league-flysystem",
        "manager"
    ],
    "license": "MIT",
    "authors": [{
        "name": "Dustin Simon"
    }],
    "autoload": {
        "psr-4": { "Dustin\\Filesystem\\": "src/" },
        "exclude-from-classmap": ["src/Test/"]
    },
    "require": {
        "php": "^8.1",
        "symfony/http-kernel": "~6.2",
        "league/flysystem": "~3.10",
        "symfony/dependency-injection": "~6.2",
        "symfony/config": "~6.2",
        "dustin/encapsulation": "~1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6"
    }
}
